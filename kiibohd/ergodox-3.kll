Name = "ergodox";
Layout = "Default";
Base = "Blank";
Version = "0.1";
Author = "Shaun Mahood";
KLL = "0.3c";
Date = "2015-09-12";
Generator = "KIICONF 0.2";

###################
### Unused Keys ###
###################

U"ESC" : flashMode();

#################
### First Row ###
#################

U"1" : U"F1";
U"2" : U"F2";
U"3" : U"F3";
U"4" : U"F4";
U"5" : U"F5";

U"8" : '<';
U"9" : '>';

##################
### Second Row ###
##################

U"W" : U"Ctrl" + U"Shift" + U"L"; # Load file in REPL 
U"E" : U"Ctrl" + U"Shift" + U"N"; # Switch REPL to NS

U"I" : '{';
U"O" : '}';

#################
### Third Row ###
#################

U"K" : '[';
U"L" : ']';

##################
### Fourth Row ###
##################

U"X" : U"Alt" + U"Shift" + U"J"; # Join s-exp
U"C" : None; 
U"V" : U"Ctrl" + U"Shift" + U"T"; # Send top form to REPL
U"B" : U"Ctrl" + U"Shift" + U"B"; # Send form before to REPL

U"COMMA" : '(';
U"PERIOD" : ')';

#################
### Fifth Row ###
#################

U"FUNCTION4" : U"Ctrl" + U"Alt" + U"K"; # Kill s-exp
U"FUNCTION5" : U"Alt" + U"Shift" + U"S"; # Split s-exp

U"DOWN" : '_';
U"UP" : U"Alt" + U"Shift" + U"0"; # Close ) and newline

######################
### Thumb Clusters ###
######################

U"HOME" : U"APP";
U"PAGEUP" : U"APP";
